{
  "name": "starter-static-pages",
  "version": "0.0.1",
  "packageManager": "pnpm@7.11.0",
  "scripts": {
    "dev": "vite --host --open",
    "build": "nr lint && nr type-check && nr build-only",
    "preview": "vite preview --port 4173",
    "test": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "commit": "git-pick && git cz",
    "up": "taze major -I",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.3.1",
    "nprogress": "^0.2.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.2",
    "@chinbor/git-pick": "^0.0.8",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.59",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog-zh-emoji": "^0.0.5",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "naive-ui": "^2.33.5",
    "sass": "^1.55.0",
    "taze": "^0.8.3",
    "typescript": "~4.8.4",
    "unocss": "^0.46.0",
    "unplugin-auto-import": "^0.11.3",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.1.8",
    "vite-plugin-pages": "^0.27.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.0.9"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
