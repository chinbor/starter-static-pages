{
  "files.eol": "\n",
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.insertSpaces": true,
  "editor.formatOnSave": false,
  "files.associations": {
    "*.css": "css"
  },
  "prettier.enable": false,
  "cSpell.words": [
    "nprogress",
    "tabbar"
  ],
}
